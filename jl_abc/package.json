{
  "name": "jl_abc",
  "version": "0.1.0",
  "description": "Renders ABC music notation",
  "author": "Romain Primet",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.12.0",
    "@jupyterlab/rendermime-interfaces": "^0.4.3",
    "@phosphor/widgets": "^1.5.0",
    "abcjs": "^3.2.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  }
}
